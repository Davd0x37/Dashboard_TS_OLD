    "@types/gulp-sourcemaps": "^0.0.32",
    "@types/node": "^10.0.2",
    "@types/webpack-stream": "^3.2.10",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "flamebearer": "latest",
    "node-sass": "^4.9.0",
    "parcel-bundler": "1.6.0",
    "tslint": "^5.9.1",
