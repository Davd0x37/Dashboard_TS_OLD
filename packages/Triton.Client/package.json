{
  "name": "@triton/client",
  "version": "1.1.0",
  "description": "Triton client",
  "repository": "git@gitlab.com:DevDigitalNomad/DashboardTS.git",
  "author": "DigitalNomad",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "parcel public/index.html -p 3030 --no-source-maps --out-dir build/app",
    "build": "parcel build public/index.html --no-source-maps --no-cache --out-dir build/app",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc app/ --target ES6 --mode file --out build/docs",
    "fix": "run-s fix:*",
    "fix:tslint": "tslint --fix --project .",
    "fix:prettier": "prettier \"app/**/*.ts\" --write",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "devDependencies": {
    "@types/graphql": "14.0.3",
    "@types/jest": "23.3.9",
    "graphql": "14.0.2",
    "jest": "23.6.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "4.1.3",
    "opn-cli": "3.1.0",
    "parcel-bundler": "1.10.3",
    "prettier": "1.14.3",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-immutable": "4.9.1",
    "typedoc": "0.13.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "apollo-boost": "0.1.19",
    "app": "^0.1.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.7.3",
    "graphql-tag": "^2.10.0",
    "lodash": "4.17.11",
    "pouchdb-adapter-http": "7.0.0",
    "pouchdb-adapter-idb": "7.0.0",
    "reflect-metadata": "0.1.12",
    "rxdb": "8.0.3",
    "rxjs": "6.3.3",
    "sweetalert2": "^7.28.11"
  },
  "alias": {
    "#": "./app",
    "#SH": "../Triton.Shared"
  }
}
