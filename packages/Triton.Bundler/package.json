{
  "name": "@triton/bundler",
  "version": "1.1.0",
  "description": "Dashboard written in TypeScript",
  "repository": "git@gitlab.com:DevDigitalNomad/DashboardTS.git",
  "author": "DigitalNomad",
  "license": "MIT",
  "main": "build/index.js",
  "bin": {
    "bundler": "build/index.js"
  },
  "private": true,
  "scripts": {
    "info": "npm-scripts-info",
    "build": "tsc",
    "dev": "tsc --watch",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc app/ --target ES6 --mode file --out build/docs",
    "fix": "run-s fix:*",
    "fix:tslint": "tslint --fix --project .",
    "fix:prettier": "prettier \"app/**/*.ts\" --write",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.2",
    "@types/webpack": "4.4.18"
  },
  "dependencies": {
    "codecov": "3.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.14",
    "happypack": "^5.0.0",
    "hard-source-webpack-plugin": "^0.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "jest": "23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "npm-run-all": "4.1.3",
    "npm-scripts-info": "^0.3.9",
    "opn-cli": "3.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.2",
    "terser-webpack-plugin": "^1.1.0",
    "ts-jest": "23.10.4",
    "ts-loader": "5.3.0",
    "ts-node": "7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-immutable": "4.9.1",
    "tslint": "5.11.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack": "^4.25.1",
    "commander": "2.19.0",
    "lodash": "4.17.11"
  }
}
