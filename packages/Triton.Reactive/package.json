{
  "name": "@triton/reactive",
  "version": "1.1.0",
  "description": "Reactive components",
  "repository": "git@gitlab.com:DevDigitalNomad/DashboardTS.git",
  "author": "DigitalNomad",
  "license": "MIT",
  "private": true,
  "bin": {
    "reactive": "build/app/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc app/ --target ES6 --mode file --out build/docs",
    "fix": "run-s fix:*",
    "fix:tslint": "tslint --fix --project .",
    "fix:prettier": "prettier \"app/**/*.ts\" --write",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.118",
    "@types/node": "10.12.2",
    "jest": "23.6.0",
    "npm-run-all": "4.1.3",
    "opn-cli": "3.1.0",
    "prettier": "1.14.3",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint-config-prettier": "1.15.0",
    "tslint-immutable": "4.9.1",
    "tslint": "5.11.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "lodash": "4.17.11"
  }
}
