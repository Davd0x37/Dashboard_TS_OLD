{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "semicolon": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-console": false,
    "no-implicit-dependencies": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "./app/@types/*"
    ]
  }
}
